    agent any
    stages{
        stage ('test' ) {
            parallel {
                stage('1st') {
                    steps{
                        echo "this is first stage"
                        sleep 30
                    }
                        
                }
                stage ('2nd') {
                    steps{
                        echo "this is 2nd stage"
                        sleep 30
                    }
                }
                stage ('3nd') {
                    steps{
                        echo "this is 3rd stage"
                        sleep 30
                    }

                }
                stage ('4nd') {
                   steps{
                        echo "this is 4th stage"
                        sleep 30
                    }

                }
                stage ('5nd') {
                    steps{
                        echo "this is 5th stage"
                        sleep 30
                    }
                }
                stage ('6th') {
                    steps{
                        echo "this is 6th stage"
                        sleep 30
                    }

                }
                stage ('7th') {
                    steps{
                        echo "this is 7th stage"
                        sleep 30
                    }

                }
            }
