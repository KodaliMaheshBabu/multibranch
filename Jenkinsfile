pipeline{
    agent any
    stages{
        stage{
            prallel{
                stage('1st') {
                    echo "this is first stage"
                    sleep 30
                }
                stage ('2nd') {
                    echo "this is secound stage"
                    sleep 30

                }
                stage ('3nd') {
                    echo "this is 3rd stage"
                    sleep 30

                }
                stage ('4nd') {
                    echo "this is 4th stage"
                    sleep 30

                }
                stage ('5nd') {
                    echo "this is 5th stage"
                    sleep 30

                }
                stage ('6th') {
                    echo "this is 6th stage"
                    sleep 30

                }
                stage ('7th') {
                    echo "this is 7th stage"
                    sleep 30

                }
            }
        }
    }
}
